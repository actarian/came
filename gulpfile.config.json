{
	"targets": {
		"browser": {
			"compile": [{
				"inputFile": "src/scss/docs.scss",
				"outputFile": "docs/css/docs.css"
}, {
				"inputFile": "src/app/main.ts",
				"outputFile": "docs/js/main.js"
			}],
			"bundle": [{
				"inputFiles": [
					"node_modules/gsap/src/uncompressed/easing/EasePack.js",
					"node_modules/gsap/src/uncompressed/TweenLite.js",
					"node_modules/gsap/src/uncompressed/TweenMax.js"
				],
				"outputFileName": "docs/js/vendors.js",
				"minify": {
					"enabled": true
				}
			}, {
				"inputFiles": [
					"docs/css/docs.css"
				],
				"outputFileName": "docs/css/docs.css",
				"minify": {
					"enabled": true
				}
			}]
		}
	},
	"options": {
		"tfs": false
	}
}
